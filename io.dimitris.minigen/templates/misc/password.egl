[%
var password = "";
var numbers = 0.to(9);
var specialChars = Sequence{"#", "$", "^", "*", "@"};
var chars = Sequence{"a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o", "p", "q", "r", "s", "t", "w", "x", "y", "z"};
var charsets = Sequence{numbers, specialChars, chars};
for (i in 1.to(18)){
	var char = charsets.random().random();
	if (chars.contains(char)) {
		if (0.to(1).random() = 1) char = char.toUpperCase();
	}
	password = password + char;
}
out.print(password);
%]